# Microsoft Azure AD Configuration (for OneDrive integration)
# Copy this file to .env and add your Azure App Registration values
VITE_AZURE_CLIENT_ID=
VITE_AZURE_TENANT_ID=

# Firebase Configuration (for Admin page + analytics)
# Get from Firebase Console > Project Settings > General > Your apps
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Sandra AI advisor API (Cloud Function direct URLs)
# Defaults to localhost emulator if not set
VITE_SANDRA_API_URL=
VITE_SANDRA_FEEDBACK_URL=
